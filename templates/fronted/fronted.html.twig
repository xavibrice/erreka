<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Erreka Inmobiliaria{% endblock %}</title>
    <link rel="icon" href="{{ asset('img/logo-erreka.jpg') }}">
    {% block stylesheets %}
        {{ encore_entry_link_tags('fronted') }}
        <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Nunito+Sans:200,300,400,700,900|Roboto+Mono:300,400,500">
        <style>
            /* GLOBAL STYLES
-------------------------------------------------- */
            /* Padding below the footer and lighter body text */

            body {
                padding-top: 3rem;
                padding-bottom: 3rem;
                color: #5a5a5a;
            }
        </style>
    {% endblock %}
</head>
<body>
    {% block header %}
        {#<header>
        <nav class="navbar navbar-expand-md navbar-light fixed-top bg-light">
            <div class="container">
                <a class="navbar-brand" href="{{ path('default') }}"><img src="{{ asset('img/logo-erreka.jpg') }}" alt=""></a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarCollapse">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" href="{{ path('default') }}">Inicio <span class="sr-only">(current)</span></a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('about_us') }}">Sobre Erreka</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Contacto</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
        </header>#}
    {% endblock %}
    {% block body %}{% endblock %}
    {% block javascripts %}
        {{ encore_entry_script_tags('fronted') }}
    {% endblock %}
</body>
</html>
