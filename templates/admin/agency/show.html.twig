{% extends 'admin/layout.html.twig' %}

{% block title %}Agency{% endblock %}

{% block content %}
    <h1>Agency</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ agency.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ agency.name }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-link" href="{{ path('agency_index') }}">Volver a la lista</a>

    <a class="btn btn-info" href="{{ path('agency_edit', {'id': agency.id}) }}">Editar</a>

    {{ include('admin/agency/_delete_form.html.twig') }}
{% endblock %}
