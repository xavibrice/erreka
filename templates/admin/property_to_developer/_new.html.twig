{{ form_start(form, {'action': path('property_to_developer_new'), 'method': 'POST'}) }}
    <div class="form-group row">
        <div class="col-md-auto">{{ form_row(form.created) }}</div>
        {% if is_granted('ROLE_ADMIN') %}
            <div class="col-md-auto">{{ form_row(form.commercial) }}</div>
        {% endif %}
        <div class="col-md-auto">{{ form_row(form.reason) }}</div>
        <div class="col-md-auto">{{ form_row(form.typeProperty) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">{{ form_row(form.zone) }}</div>
        <div class="col-md-auto">{{ form_row(form.street) }}</div>
        <div class="col-md-auto">{{ form_row(form.portal) }}</div>
        <div class="col-md-auto">{{ form_row(form.floor) }}</div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">{{ form_row(form.full_name) }}</div>
        <div class="col-md-auto">{{ form_row(form.mobile) }}</div>
        <div class="col-md-auto">{{ form_row(form.phone) }}</div>
        <div class="col-md-auto">{{ form_row(form.url) }}</div>
    </div>
    <div class="form-group">
        <div class="col-md-auto">{{ form_row(form.comment) }}</div>
    </div>
    <button class="btn btn-success">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}