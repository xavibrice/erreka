{{ form_start(form, {'action': path('news_new'), 'method': 'POST'}) }}
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {% if is_granted('ROLE_ADMIN') %}
                {{ form_row(form.commercial) }}
            {% endif %}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.created) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.zone) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.street) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.situation) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.reason) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.portal) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.floor) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.firstName) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.lastName) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.mobile) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.phone) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-6 mb-3 mb-sm-0">
            {{ form_row(form.email) }}
        </div>
        <div class="col-sm-6">
            {{ form_row(form.price) }}
        </div>
    </div>
    <div class="form-group">
        {{ form_row(form.url) }}
    </div>
    <div class="form-group">
        {{ form_row(form.comment) }}
    </div>
<button class="btn btn-success m-3">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}