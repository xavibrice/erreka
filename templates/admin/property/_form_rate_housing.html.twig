{#{{ dump(form) }}#}

{{ form_start(form) }}
    <div class="form-group row">
        <div class="col-sm-4 mb-3 mb-sm-0">
            {{ form_row(form.visited) }}
        </div>
        <div class="col-sm-4">
            {{ form_row(form.min_price) }}
        </div>
        <div class="col-sm-4">
            {{ form_row(form.max_price) }}
        </div>
    </div>
    <div class="form-group">
        {{ form_row(form.comment) }}
    </div>
    <div class="form-group row">
        <div class="col-sm-4 mb-3 mb-sm-0">
            {{ form_row(form.bedrooms) }}
        </div>
        <div class="col-sm-4">
            {{ form_row(form.bathrooms) }}
        </div>
        <div class="col-sm-4">
            {{ form_row(form.real_meters) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-3 mb-3 mb-sm-0">
            {{ form_row(form.terrace) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.suit_bathroom) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.video_intercom) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.storage_room) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-3 mb-3 mb-sm-0">
            {{ form_row(form.direct_garage) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.disabled_access) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.zero_dimension) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.elevator) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-3 mb-3 mb-sm-0">
            {{ form_row(form.bathroomState) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.beenCooking) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.windowsState) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.beenWalls) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-sm-3 mb-3 mb-sm-0">
            {{ form_row(form.doorsState) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.groundState) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.window) }}
        </div>
        <div class="col-sm-3">
            {{ form_row(form.heating) }}
        </div>
    </div>
    <button class="btn btn-success">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}
