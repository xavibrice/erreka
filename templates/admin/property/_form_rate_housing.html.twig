{#{{ dump(form) }}#}

{{ form_start(form) }}
    <div class="row bg-light p-3 m-3">
        <div class="col-md-12 text-center">
            <strong>INFORME</strong>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.visited) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.min_price) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.max_price) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            {{ form_row(form.comment) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.bedrooms) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.exteriorBedrooms) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.patioBedrooms) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.bathrooms) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.exteriorBathrooms) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.bathroomState) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.exteriorCooking) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.beenCooking) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.door) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.doorsState) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.window) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.windowsState) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.wall) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.beenWalls) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.ground) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.groundState) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.heating) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.orientation) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.real_meters) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.energyCertificate) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.energyConsumption) }} kWh/m2 a√±o
        </div>
    </div>
    <div class="row bg-light p-3 m-3">
        <div class="col-md-12 text-center">
            <strong>EXTRAS</strong>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.terrace) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.balcony) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.storage_room) }}
        </div>

        <div class="col-md-auto">
            {{ form_row(form.direct_garage) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.disabled_access) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.zero_dimension) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.pets) }}
        </div>
    </div>
    <div class="row bg-light p-3 m-3">
        <div class="col-md-12 text-center">
            <strong>EDIFICIO</strong>
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.elevator) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.antiquity) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.communityExpenses) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.amountPendingSpills) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.buildingStructure) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            {{ form_row(form.pendingSpills) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-6">
            {{ form_row(form.spillsFuture) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.administrator) }}
        </div>
        <div class="col-md-auto">
            {{ form_row(form.mobileAdministrator) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {{ form_row(form.image) }}
        </div>
    </div>
    <div class="form-group row">
        <div class="col-md-auto">
            {% for image in form.vars.data.image %}
                <img src="{{ uploaded_asset(image.imagePath)|imagine_filter('squared_thumbnail_small') }}">
            {% endfor %}
        </div>
    </div>
    <button class="btn btn-success">{{ button_label|default('Guardar') }}</button>
{{ form_end(form) }}
